{
  "name": "vael-lens",
  "displayName": "lens by vael",
  "version": "1.0.1",
  "description": "Privacy-first browser extension that collects browsing data locally to enable AI-powered insights",
  "author": "Vael AI",
  "scripts": {
    "dev": "plasmo dev",
    "build": "pnpm exec prettier --write --log-level=silent . && plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/messaging": "0.6.2",
    "@plasmohq/storage": "1.15.0",
    "@radix-ui/react-accordion": "1.2.3",
    "@radix-ui/react-alert-dialog": "1.1.6",
    "@radix-ui/react-dialog": "1.1.6",
    "@radix-ui/react-scroll-area": "1.2.3",
    "@radix-ui/react-separator": "1.1.2",
    "@radix-ui/react-slot": "1.1.2",
    "@radix-ui/react-switch": "1.1.3",
    "@radix-ui/react-tabs": "1.1.3",
    "@radix-ui/react-toggle": "1.1.2",
    "@radix-ui/react-tooltip": "1.1.8",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "json-edit-react": "1.25.0",
    "lucide-react": "0.484.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "3.0.2",
    "tailwindcss": "3.4.1",
    "tailwindcss-animate": "1.0.7",
    "typescript": "5.7.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/uuid": "^10.0.0",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "tsx": "4.19.3"
  },
  "manifest": {
    "name": "lens by vael",
    "description": "Privacy-first browser extension that collects browsing data locally to enable AI-powered insights",
    "permissions": [
      "storage",
      "background",
      "activeTab",
      "notifications"
    ],
    "host_permissions": [
      "http://localhost:3000/",
      "https://lens.vael.ai/"
    ],
    "background": {
      "service_worker": "background.ts"
    },
    "options_ui": {
      "page": "options.html",
      "open_in_tab": true
    },
    "action": {
      "default_popup": "popup.html"
    },
    "web_accessible_resources": [
      {
        "resources": [
          "data-viewer.html"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ]
  }
}
